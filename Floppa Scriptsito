
Misc:AddButton({
    Name = "Enable Rollback",
    Callback = function()
        while task.wait() do
            local ohTable1 = {
                ["1"] = "\255"
            }
            game:GetService("ReplicatedStorage").Remotes.Data.UpdateHotbar:FireServer(ohTable1)
            print("Rollback Setup")
        end
    end
})
